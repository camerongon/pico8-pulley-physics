function _init()
m1={x=52,y=80,inity,mass=flr(rnd(49))+1,s=1}
m1.inity=m1.y
m2={x=68,y=80,inity,mass=flr(rnd(49))+1,s=1}
m2.inity=m2.y
wheel={x=56,y=24,s=32,frame=0}
gravity=9.8
a=((m1.mass*gravity)-(m2.mass*gravity))/(m1.mass+m2.mass)
t=0
end
-->8
function _update()

if m1.y<120 and m1.y >40 then
	t+=1/30
	m1.y=m1.inity+(a/2)*(t^2)
	m2.y=m2.inity-(a/2)*(t^2)
elseif m1.mass>m2.mass then
	m1.y=120
	m2.y=40
else
	m1.y=40
	m2.y=120
end

if btn(❎) then
	m1={x=52,y=80,inity,mass=flr(rnd(19))+1,s=1}
	m1.inity=m1.y
	m2={x=68,y=80,inity,mass=flr(rnd(19))+1,s=1}
	m2.inity=m2.y
	wheel={x=56,y=24,s=32,frame=0}
	gravity=9.8
	a=((m1.mass*gravity)-(m2.mass*gravity))/(m1.mass+m2.mass)
	t=0
end

end
-->8
function _draw()
cls()
map()
spr(wheel.s,wheel.x,wheel.y)
spr(wheel.s+1,wheel.x+8,wheel.y)
spr(wheel.s+16,wheel.x,wheel.y+8)
spr(wheel.s+17,wheel.x+8,wheel.y+8)
spr(m1.s,m1.x,m1.y)
spr(m2.s,m2.x,m2.y)
line(56,35,m1.x+4,m1.y,9)
line(71,35,m2.x+3,m2.y,9)
print("40m",27,105,8)
print("40m",90,65,8)
print("left mass: "..m1.mass.."kg",0,0,9)
print("right mass: "..m2.mass.."kg",0,8,9)
print(t.."s",0,16,8)
v=(abs(m1.y-m1.inity))/t
rectfill(1,121, 4,121-v*2, 9)
print("velocity",0,123,8)
print("press",109,111,11)
print("❎ to",109,117,11)
print("reset",109,123,11)
end
